{
  "name": "angular2-boilerplate",
  "version": "0.1.4",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/common": "4.0.0-beta.2",
    "@angular/core": "4.0.0-beta.2",
    "@angular/http": "4.0.0-beta.2",
    "@angular/platform-browser": "4.0.0-beta.2",
    "@angular/platform-browser-dynamic": "4.0.0-beta.2",
    "@angular/platform-webworker": "4.0.0-beta.2",
    "@angular/platform-webworker-dynamic": "4.0.0-beta.2",
    "@angular/router": "3.4.4",
    "@types/systemjs": "^0.19.33",
    "core-js": "2.4.1",
    "rxjs": "5.0.3",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@angular/compiler": "4.0.0-beta.2",
    "@angular/compiler-cli": "4.0.0-beta.2",
    "@angular/platform-server": "4.0.0-beta.2",
    "@angularclass/hmr-loader": "3.0.2",
    "@types/requirejs": "^2.1.28",
    "add-asset-html-webpack-plugin": "1.0.2",
    "angular-router-loader": "0.5.0",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "compression-webpack-plugin": "0.3.2",
    "css-loader": "0.26.1",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.26.0",
    "jade": "1.11.0",
    "jade-html-loader": "0.0.3",
    "node-static": "0.7.9",
    "npm-run-all": "4.0.1",
    "raw-loader": "0.5.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "to-string-loader": "1.1.5",
    "typescript": "2.1.5",
    "webpack": "v2.2.0-rc.3",
    "webpack-bundle-analyzer": "2.2.1",
    "webpack-dev-server": "v2.2.0-rc.0",
    "webpack-error-notification": "0.1.6",
    "webpack-merge": "2.4.0"
  },
  "scripts": {
    "start": "DLL=true webpack-dev-server --progress --inline --hot",
    "start:webworker": "run-p start:webworker:*",
    "start:webworker:static": "static www",
    "start:webworker:webworker": "DLL=true webpack --watch --progress --config ./config/webworker.config.js",
    "start:webworker:interface": "DLL=true webpack --watch --progress --config ./config/interface.config.js",
    "clear": "rm -rf www && rm -rf .compiled && mkdir .compiled;",
    "build": "npm run build:dll && npm run build:webworker:prod && npm run build:interface:prod",
    "build:aot": "ngc -p tsconfig.aot.json",
    "build:aot:frontend": "AOT=true webpack",
    "build:aot:frontend:prod": "NODE_ENV='production' AOT=true webpack",
    "build:dll": "NODE_ENV='production' webpack --config ./config/dll.config.js",
    "build:interface": "DLL=true webpack --config ./config/interface.config.js",
    "build:interface:prod": "DLL=true NODE_ENV='production' webpack --config ./config/interface.config.js",
    "build:webworker": "DLL=true webpack --config ./config/webworker.config.js",
    "build:webworker:prod": "DLL=true NODE_ENV='production' webpack --config ./config/webworker.config.js"
  }
}
