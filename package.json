{
  "name": "angular2-boilerplate",
  "version": "0.1.4",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/common": "4.0.0-beta.2",
    "@angular/core": "4.0.0-beta.2",
    "@angular/http": "4.0.0-beta.2",
    "@angular/platform-browser": "4.0.0-beta.2",
    "@angular/platform-browser-dynamic": "4.0.0-beta.2",
    "@angular/router": "3.4.1",
    "core-js": "2.4.1",
    "rxjs": "5.0.2",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@angular/compiler": "4.0.0-beta.2",
    "@angular/compiler-cli": "4.0.0-beta.2",
    "@angular/platform-server": "4.0.0-beta.2",
    "@types/requirejs": "2.1.28",
    "@types/systemjs": "0.19.33",
    "@types/webpack": "2.1.0",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "angular-router-loader": "0.4.0",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "3.0.0-beta.17",
    "compression-webpack-plugin": "0.3.2",
    "css-loader": "0.26.1",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.26.0",
    "jade": "1.11.0",
    "jade-html-loader": "0.0.3",
    "npm-run-all": "4.0.0",
    "raw-loader": "0.5.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.4.0",
    "to-string-loader": "1.1.5",
    "typescript": "2.1.4",
    "webpack": "v2.2.0-rc.3",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-server": "v2.2.0-rc.0",
    "webpack-error-notification": "0.1.6",
    "webpack-merge": "2.3.1"
  },
  "scripts": {
    "postinstall": "npm run build:clear && npm run build:dll",
    "start": "DLL=true webpack-dev-server --progress --inline --hot",
    "build": "npm run build && run build:application:prod",
    "build:clear": "rm -rf www && rm -rf .compiled && mkdir .compiled;",
    "build:dll": "NODE_ENV='production' webpack --config ./config/dll.config.js",
    "build:application": "webpack",
    "build:dll:prod": "NODE_ENV='production' webpack --config ./config/dll.config.js",
    "build:application:prod": "NODE_ENV='production' webpack",
    "build:aot": "ngc -p tsconfig.aot.json",
    "build:aot:application": "AOT=true webpack",
    "build:aot:application:prod": "NODE_ENV='production' AOT=true webpack"
  }
}
